<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/colorPrimaryDark"
    android:weightSum="4"
    tools:context="com.example.asus_pc.sightmark.FifthActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1">

        <Button
            android:id="@+id/button1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:textAlignment="center"
            android:text="create"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="3">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/button2"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/ic_close_black_24dp"
                android:textAlignment="center" />

            <Button
                android:id="@+id/button24"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/button1"
                android:textAlignment="center" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/button3"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/ic_close_black_24dp"
                android:textAlignment="center" />

            <Button
                android:id="@+id/button25"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignTop="@+id/button24"
                android:layout_centerHorizontal="true"
                android:layout_weight="1"
                android:text=""
                android:textAlignment="center" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/button4"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/ic_close_black_24dp"
                android:textAlignment="center" />

            <Button
                android:id="@+id/button26"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text=""
                android:textAlignment="center" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:weightSum="3"
        android:layout_weight="1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="1">

            <ImageButton
                android:id="@+id/button5"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/ic_close_black_24dp"
                android:textAlignment="center" />

            <Button
                android:id="@+id/button27"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignLeft="@+id/button24"
                android:layout_alignStart="@+id/button24"
                android:layout_below="@+id/button24"
                android:textAlignment="center"
                android:text="" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="1">

            <ImageButton
                android:id="@+id/button6"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/ic_close_black_24dp"
                android:textAlignment="center" />

            <Button
                android:id="@+id/button28"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignLeft="@+id/button25"
                android:layout_alignStart="@+id/button25"
                android:layout_alignTop="@+id/button27"
                android:textAlignment="center"
                android:text="" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="1">

            <ImageButton
                android:id="@+id/button7"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/ic_close_black_24dp"
                android:textAlignment="center" />

            <Button
                android:id="@+id/button29"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignLeft="@+id/button26"
                android:layout_alignStart="@+id/button26"
                android:layout_alignTop="@+id/button28"
                android:textAlignment="center"
                android:text="" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:weightSum="3"
        android:layout_weight="1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="1">

            <ImageButton
                android:id="@+id/button8"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/ic_close_black_24dp"
                android:textAlignment="center" />

            <Button
                android:id="@+id/button30"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignLeft="@+id/button27"
                android:layout_alignParentBottom="true"
                android:layout_alignStart="@+id/button27"
                android:textAlignment="center"
                android:text="" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="1">

            <ImageButton
                android:id="@+id/button9"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/ic_close_black_24dp"
                android:textAlignment="center" />

            <Button
                android:id="@+id/button31"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignLeft="@+id/button28"
                android:layout_alignStart="@+id/button28"
                android:layout_alignTop="@+id/button30"
                android:textAlignment="center"
                android:text="" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="1">

            <ImageButton
                android:id="@+id/button10"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/ic_close_black_24dp"
                android:textAlignment="center" />

            <Button
                android:id="@+id/button32"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignLeft="@+id/button29"
                android:layout_alignStart="@+id/button29"
                android:layout_alignTop="@+id/button31"
                android:textAlignment="center"
                android:text="" />

        </LinearLayout>

    </LinearLayout>

</LinearLayout>
